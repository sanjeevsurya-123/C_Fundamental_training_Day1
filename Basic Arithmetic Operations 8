#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int num1,num2;
    scanf("%d",&num1);
    scanf("%d",&num2);

    printf("Sum: %d\n",num1+num2);
    printf("Difference: %d\n",num1-num2);
    printf("Product: %d\n",num1*num2);
    printf("Quotient: %d\n",num1/num2);
    printf("Remainder: %d",num1%num2);
    return 0;
}
